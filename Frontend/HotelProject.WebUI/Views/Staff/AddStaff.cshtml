@model AddStaffViewModel
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Staff Member Name</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
    <label>Staff Member Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Secial Media 1</label>
    <input type="text" asp-for="SocialMedia1" class="form-control" />
    <br />
    <label>Secial Media 2</label>
    <input type="text" asp-for="SocialMedia2" class="form-control" />
    <br />
    <label>Secial Media 3</label>
    <input type="text" asp-for="SocialMedia3" class="form-control" />
    <br />
    <button class="btn mb-1 btn-rounded btn-outline-info">Add Staff Member</button>
</form>

